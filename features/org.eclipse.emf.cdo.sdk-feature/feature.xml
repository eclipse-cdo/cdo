         id="org.eclipse.emf.cdo.server.product"
         version="0.0.0"/>

   <includes
         id="org.eclipse.emf.cdo.server.source"
         version="0.0.0"/>

   <!-- Examples -->
   <includes
         id="org.eclipse.emf.cdo.examples"
         version="0.0.0"/>

   <includes
         id="org.eclipse.emf.cdo.examples.source"
         version="0.0.0"/>

   <!-- Documentation -->
   <includes
         id="org.eclipse.emf.cdo.doc"
         version="0.0.0"/>
         
